{
    "name": "wazemaki/yii2-settings-handler",
    "description": "Flexible database-backed settings handler for Yii2 applications with admin UI",
    "type": "yii2-extension",
    "keywords": ["yii2", "settings", "configuration", "database", "admin"],
    "license": "MIT",
    "authors": [
        {
            "name": "Wazemaki",
            "homepage": "https://github.com/wazemaki"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap5": "~2.0.0",
        "kartik-v/yii2-widget-select2": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "wazemaki\\settings\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
